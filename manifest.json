{
  "manifest_version": 3,
  "name": "Bookmarker",
  "version": "1.0",
  "description": "Bookmark pages with priority and manage them in a ranked list.",
  "permissions": ["storage", "tabs"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}